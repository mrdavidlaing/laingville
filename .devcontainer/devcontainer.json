{
  "name": "Laingville",
  "image": "ghcr.io/mrdavidlaing/laingville/laingville-devcontainer:latest",
  "remoteUser": "vscode",
  "mounts": [
    "source=${env:SSH_AUTH_SOCK},target=/ssh-agent,type=bind"
  ],
  "remoteEnv": {
    "SSH_AUTH_SOCK": "/ssh-agent"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "jnoortheen.nix-ide"
      ]
    }
  }
}
